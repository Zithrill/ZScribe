[MASTER]
   ignore=CVS
   ignore-patterns=
   persistent=yes
   load-plugins=
   jobs=1
   unsafe-load-any-extension=no
   extension-pkg-whitelist=

   [MESSAGES CONTROL]
   confidence=
   disable=C0111

   [REPORTS]
   output-format=text
   files-output=no
   reports=yes
   evaluation=10.0 - ((float(5 * error + warning + refactor + convention) / statement) * 10)

   [VARIABLES]
   init-import=no
   dummy-variables-rgx=_$|dummy

   [FORMAT]
   indent-string='    '
   max-line-length=100

   [LOGGING]
   logging-modules=logging

   [MISCELLANEOUS]
   notes=FIXME,XXX,TODO

   [SIMILARITIES]
   min-similarity-lines=4
   ignore-comments=yes
   ignore-docstrings=yes
   ignore-imports=no